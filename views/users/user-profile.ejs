<% include ../partials/bootstrap_boilerplate %>




<div class="profileContainer">
<div class="container">
      <div class="row">
      <div class="col-md-5  toppad  pull-right col-md-offset-3 ">

      </div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
   
   
          <div class="panel panel-info">
            <div class="panel-heading">
              <h3 class="panel-title"><%= userInfo.first_name %> <%= userInfo.last_name %></h3>
            </div>
            <div class="panel-body">
              <div class="row">
                <div class="col-md-3 col-lg-3 " align="center"> <img alt="User Pic" src="data:image/png;base64," class="img-circle img-responsive"> </div>
                
        
                <div class=" col-md-9 col-lg-9 "> 
                  <table class="table table-user-information">
                    <tbody>
                      <tr>
                        <td>User Name:</td>
                        <td><%= userInfo.username %></td>
                      </tr>
                      <tr>
                        <td>First Name:</td>
                        <td><%= userInfo.first_name %></td>
                      </tr>
                      <tr>
                        <td>Last Name:</td>
                        <td><%= userInfo.last_name %></td>
                      </tr>
                   
                         <tr>
                       
                     
                      <tr>
                        <td>Email:</td>
                        <td><a href="mailto:<%= userInfo.email %>"><%= userInfo.email %></a></td>
                      </tr>
                       
                           
                      </tr>
                     
                    </tbody>
                  </table>
                  
                  <span class="pull-right">
                  <a href="/" class="btn btn-primary">Go To Home Page</a>
                  </span>
                </div>
              </div>
            </div>
                 <div class="panel-footer">
                      Your Profile Information
                    </div>
            
          </div>
        </div>
      </div>
    </div>
    </div>